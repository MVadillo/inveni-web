  <body class="hold-transition login-page-electron">
    <div class="login-box">
      <div class="login-logo">
        <!--<a href="#" class="h1"><img src="../assets/dist/img/logo.png" alt="Inveni Logo" ></a>-->
      </div>
      <!-- /.login-logo -->
      <div class="card">
        <div class="card-body login-card-body">
          <p class="login-box-msg">Login</p>

          <form action="#">
            <div class="input-group mb-3">
              <input id="input_usuario" type="text" class="form-control" placeholder="usuario">
              <div class="input-group-append">
                <div class="input-group-text">
                  <span class="fas fa-user"></span>
                </div>
              </div>
            </div>
            <div class="input-group mb-3">
              <input id="input_password" type="password" class="form-control" placeholder="contraseña">
              <div class="input-group-append">
                <div class="input-group-text">
                  <span class="fas fa-lock"></span>
                </div>
              </div>
            </div>
            <!-- <div class="row">
              <div class="col-8">
                <div class="icheck-primary">
                  <input id="input_recordarme" type="checkbox" id="remember">
                  <label for="remember">
                    Mantener sesión iniciada
                  </label>
                </div>
              </div>
              <div class="col-4">
                
              </div> 
            </div> -->
            <div class="row">
              <div class="col-12">
                <span class="login_status_msj" id="login_status"></span>
              </div>
            </div>
          </form>
    
          <div class="social-auth-links text-center mb-3">
            <button id="btn_iniciar_sesion" class="btn btn-primary btn-block">Iniciar sesión</button>
          </div>
          <!-- /.social-auth-links -->
        </div>
        <!-- /.login-card-body -->
      </div>
    </div>
    <!-- /.login-box -->
